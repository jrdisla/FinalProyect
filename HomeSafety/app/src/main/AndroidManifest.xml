<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jdisa.homesafety">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ssh"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Autenticacion.LoginActivity"

            android:theme="@style/AppTheme.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Autenticacion.SignupActivity"

            android:theme="@style/AppTheme.NoActionBar"/>

        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            />
        <activity android:name=".Autenticacion.ResetPasswordActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Menu.Activity.AboutUsActivity"

            android:theme="@style/AppTheme.NoActionBar"> </activity>
    </application>

</manifest>